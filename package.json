{
  "name": "log4vue",
  "version": "0.0.2",
  "description": "Vue.js logger",
  "author": "Ricardo Vega",
  "private": false,
  "license": "MIT",
  "main": "dist/log4vue.common.js",
  "module": "dist/log4vue.esm.js",
  "unpkg": "dist/log4vue.js",
  "jsdelivr": "dist/log4vue.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ricveal/log4vue"
  },
  "files": [
    "dist/*.js"
  ],
  "keywords": [
    "vue",
    "log",
    "logger"
  ],
  "scripts": {
    "dev": "node examples/server.js",
    "dev:dist": "rollup -wm -c build/rollup.dev.config.js",
    "build": "node build/build.js",
    "lint": "eslint src examples",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest --config test/unit/jest.conf.js --coverage",
    "docs": "cd docs && gitbook install && gitbook serve",
    "docs:deploy": "bash ./build/update-docs.sh",
    "release": "bash build/release.sh",
    "commit": "git-cz"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow-vue": "^1.0.0",
    "buble": "^0.19.3",
    "codecov": "^3.0.1",
    "commitizen": "^2.9.6",
    "css-loader": "^0.28.10",
    "es6-promise": "^4.2.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-vue-libs": "^2.1.0",
    "express": "^4.16.2",
    "express-urlrewrite": "^1.2.0",
    "gitbook-plugin-edit-link": "^2.0.2",
    "gitbook-plugin-github": "^3.0.0",
    "gitbook-plugin-theme-vuejs": "^1.1.0",
    "jest": "^22.4.3",
    "jest-serializer-vue": "^1.0.0",
    "mockdate": "^2.0.2",
    "path-to-regexp": "^1.7.0",
    "rollup": "^0.56.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-watch": "^4.0.0",
    "uglify-js": "^3.3.13",
    "vue": "^2.5.16",
    "vue-jest": "^2.5.0",
    "vue-loader": "^13.7.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^1.12.2"
  },
  "bugs": {
    "url": "https://github.com/ricveal/log4vue/issues"
  },
  "homepage": "https://github.com/ricveal/log4vue#readme"
